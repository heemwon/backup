{
  "name": "hello-world",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "emul": "firebase emulators:start",
    "lint:js": "eslint --ext \".js,.vue\" --ignore-path .gitignore .",
    "lint:style": "stylelint \"**/*.{vue,css}\" --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "format": "npx prettier --write \"{,!(node_modules|.nuxt|dist)/**/}*.{js,vue}\"",
    "test": "jest --watchAll",
    "docker:front": "docker exec -it pxd.co.kr bash",
    "docker:fe": "docker exec -it firebase-emulators bash",
    "docker:build": "docker-compose up --build ",
    "docker:start": "docker-compose up -d frontend firebase-emulators",
    "docker:stop": "docker-compose stop frontend firebase-emulators",
    "deploy:preview": "docker-compose run --rm netlify-cli deploy",
    "deploy:prod": "docker-compose run --rm netlify-cli deploy --prod"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.13.6",
    "@nuxtjs/firebase": "^7.6.1",
    "@nuxtjs/i18n": "^7.2.0",
    "@nuxtjs/redirect-module": "^0.3.1",
    "@nuxtjs/robots": "^2.5.0",
    "@nuxtjs/sitemap": "^2.4.0",
    "aos": "^2.3.4",
    "axios": "^0.21.3",
    "bodymovin": "^4.13.0",
    "core-js": "^3.15.1",
    "crypto-js": "^4.1.1",
    "firebase": "^8.10.0",
    "jquery": "^3.6.0",
    "nuxt": "^2.15.7",
    "vee-validate": "^3.4.13",
    "vue-body-scroll-lock": "^2.0.2",
    "vue-lottie": "^0.2.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.14.7",
    "@nuxtjs/eslint-config": "^6.0.1",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@nuxtjs/storybook": "^4.1.1",
    "@nuxtjs/style-resources": "^1.2.1",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@vue/test-utils": "^1.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^27.0.5",
    "eslint": "^7.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "eslint-plugin-vue": "^7.12.1",
    "jest": "^27.0.5",
    "nuxt-vite": "^0.2.1",
    "prettier": "^2.3.2",
    "sass": "^1.39.0",
    "sass-loader": "^10.2.0",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "vue-jest": "^3.0.4"
  }
